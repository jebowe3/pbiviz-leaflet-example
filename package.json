{
  "name": "powerbi-leaflet-visual",
  "version": "1.0.0",
  "description": "Choropleth and point-map visual for Power BI using Leaflet",
  "license": "MIT",
  "scripts": {
    "start": "pbiviz start",
    "build": "pbiviz package",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,json,css,less}\"",
    "test": "jest --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.0",
    "@types/lodash.debounce": "^4.0.9",
    "core-js": "^3.30.0",
    "d3": "^7.9.0",
    "fast-deep-equal": "^3.1.3",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "lodash.debounce": "^4.0.8",
    "powerbi-visuals-api": "^5.0.0",
    "powerbi-visuals-utils-dataviewutils": "^6.1.0",
    "simple-statistics": "^7.8.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/d3": "^7.4.1",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^29.5.0",
    "@types/leaflet": "^1.9.4",
    "@types/leaflet.markercluster": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^8.10.0",
    "glob": "^11.0.2",
    "husky": "^9.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^16.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint -- --fix",
      "npm run format"
    ]
  },
  "overrides": {
    "rimraf": "^5.0.10",
    "glob": "^11.0.2"
  }
}
